{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
      "object-shorthand": 0,
      "import/prefer-default-export": 0,
      "react/prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/no-array-index-key": 0,
      "class-methods-use-this": 0,
      "no-plusplus": 0,
      "no-func-assign": 0,
      "no-console": [
          "error",
          {
            allow: [
              "warn",
              "error"
            ]
          }
        ],
  }
}
